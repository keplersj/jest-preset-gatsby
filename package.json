{
  "name": "jest-preset-gatsby",
  "version": "0.0.0-development",
  "description": "Preset to simplify testing Gatsby sites with Jest",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keplersj/jest-preset-gatsby.git"
  },
  "keywords": [
    "gatsby",
    "jest",
    "unit-testing",
    "typescript",
    "testing"
  ],
  "author": "Kepler Sticka-Jones <kepler@stickajones.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keplersj/jest-preset-gatsby/issues"
  },
  "homepage": "https://github.com/keplersj/jest-preset-gatsby#readme",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "babel-jest": "^25.1.0",
    "babel-preset-gatsby": "^0.2.8",
    "identity-obj-proxy": "^3.0.0",
    "jest-raw-loader": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-jest": "^23.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^4.0.0",
    "jest": "^25.1.0",
    "jest-runner-eslint": "^0.7.4",
    "jest-runner-prettier": "^0.3.1",
    "prettier": "^1.18.2",
    "semantic-release": "^16.0.0"
  }
}

{
  "name": "jest-preset-gatsby",
  "version": "0.0.0-development",
  "description": "Preset to simplify testing Gatsby sites with Jest",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keplersj/jest-preset-gatsby.git"
  },
  "keywords": [
    "gatsby",
    "jest",
    "unit-testing",
    "typescript",
    "testing"
  ],
  "author": "Kepler Sticka-Jones <kepler@stickajones.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keplersj/jest-preset-gatsby/issues"
  },
  "homepage": "https://github.com/keplersj/jest-preset-gatsby#readme",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.15.0",
    "babel-jest": "^27.0.0",
    "babel-preset-gatsby": "^2.17.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-raw-loader": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "eslint": "^7.32.0",
    "eslint-config-starstuff": "^1.4.42",
    "husky": "^7.0.0",
    "jest": "^27.0.4",
    "jest-runner-eslint": "^0.10.0",
    "jest-runner-prettier": "^0.3.6",
    "prettier": "^2.0.5",
    "semantic-release": "^17.4.5"
  },
  "eslintConfig": {
    "extends": "starstuff/auto",
    "env": {
      "node": true
    }
  }
}
